
@{
    ViewBag.Title = "New";
}

<h2>New Teacher</h2>

<form method="POST" action="/Teacher/Create">
    <div id="block">
        <label id="label">First Name</label>
        <input type="text" name="TeacherFname"/>
    </div>

    <div id="block">
        <label id="label">Last Name</label>
        <input type="text" name="TeacherLname"/>
    </div>

    <div id="block">
        <label>Employee Number</label>
        <input type="text" name="TeacherNumber" />
    </div>

    <div id="block">
        <label id="label1">Salary</label>
        <input type="text" name="TeacherSalary">
    </div>
    <input type="submit" value="Add">
</form>

